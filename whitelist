local function updateCharacterStats()
	player = Players.LocalPlayer
	character = player.Character or player.CharacterAdded:Wait() -- 캐릭터가 없으면 새로 로드될 때까지 대기
end

function CheckHWID(hwidval)
for _,whitelisted in pairs(WhitelistedHWIDs) do
 if hwidval == whitelisted then
     return true
 elseif hwidval ~= whitelisted then
     return false
       end
    end
end

if not CheckHWID(hwid) then
    game.Players.LocalPlayer:Kick("당신은 화이트리스트에 등록되었지않습니다! 화이트리스트 등록은 Crystalized : Cosmic 디스코드 채널에서 해주세요!")
    return
end
