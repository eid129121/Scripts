function ToggleClickKill()
    if not ClickKillon then
        ClickKillon = true

        local plr = game.Players.LocalPlayer
        local mouse = plr:GetMouse()
        local rs = game:GetService('ReplicatedStorage')
        Click = mouse.Button1Down:Connect(function()
            local model = mouse.Target:FindFirstAncestorOfClass('Model')
            if model then
                local clickedPlayer = Players:GetPlayerFromCharacter(model)
                if clickedPlayer then
                    rs.CarbonResource.Events:GetChildren()[4]:FireServer(clickedPlayer.Character.Humanoid, 100, "Head", {
                        'nil',
                        'Auth',
                        'nil',
                        'nil'
                    })
                end
            end
        end)
    else
        ClickKillon = false
        if Click then
            Click:Disconnect()
            Click = nil
        end
    end
end
function ToggleInfiniteJump()
    if not InfiniteJumpon then
        InfiniteJumpon = true
        _G.infinJumpStarted = true

        --The actual infinite jump
        local plr = game:GetService('Players').LocalPlayer
        local m = plr:GetMouse()
        Stepped1 = m.KeyDown:connect(function(k)
            if true then
                if k:byte() == 32 then
                humanoid = game:GetService'Players'.LocalPlayer.Character:FindFirstChildOfClass('Humanoid')
                humanoid:ChangeState('Jumping')
                wait()
                humanoid:ChangeState('Seated')
                end
            end
        end)
    else
        InfiniteJumpon = false
        if Stepped1 then
            Stepped1:Disconnect()
            Stepped1 = nil
        end
    end
end
local mouse = game.Players.LocalPlayer:GetMouse() 
local plr = game.Players.LocalPlayer 
local torso = plr.Character.Head
local deb = true 
local ctrl = {f = 0, b = 0, l = 0, r = 0} 
local lastctrl = {f = 0, b = 0, l = 0, r = 0} 
local maxspeed = 400 
local speed = 5000 

function ToggleFly()
    if not Flyon then
        Flyon = true
        Fly()
    else
        Flyon = false
        Fly()
    end
end

function ToggleNoclip()
	if not Clipon then
		Clipon = true
        -- print("Noclip: true")
		Stepped = RunService.Stepped:Connect(function()
			for _, b in pairs(Workspace:GetChildren()) do
				if b.Name == player.Name then
					for _, v in pairs(b:GetChildren()) do
						if v:IsA("BasePart") then
							v.CanCollide = false
						end
					end
				end
			end
		end)
	else
		Clipon = false
        -- print("Noclip: off")
		if Stepped then
			Stepped:Disconnect()
			Stepped = nil
		end
		RunService.Heartbeat:Wait() -- Ensure that the changes apply properly
		for _, b in pairs(Workspace:GetChildren()) do
			if b.Name == player.Name then
				for _, v in pairs(b:GetChildren()) do
					if v:IsA("BasePart") then
						v.CanCollide = true
					end
				end
			end
		end
	end
end

local function highlightPlayers()
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer then
            -- 해당 플레이어의 캐릭터와 팀을 가져옵니다
            local character = player.Character
            local team = player.Team
            
            if character and team then
                -- 기존 하이라이트 객체가 있으면 제거합니다
                local existingHighlight = character:FindFirstChildOfClass("Highlight")
                if existingHighlight then
                    existingHighlight:Destroy()
                end
                
                -- 하이라이트 객체를 생성하고 설정합니다
                local highlight = Instance.new("Highlight")
                highlight.Parent = character
                if team then
                    highlight.FillColor = team.TeamColor.Color
                    highlight.OutlineColor = team.TeamColor.Color -- 팀 색상으로 외곽선 설정
                else
                    highlight.FillColor = Color3.new(1, 1, 1)
                    highlight.OutlineColor = Color3.new(1, 1, 1)
                end
            end
        end
    end
end



