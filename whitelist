local Section = combat:CreateSection("- ESP")
local Button = combat:CreateButton({
    Name = "Chatgpt ESP",
    CallBack = function()
        Players.PlayerAdded:Connect(function(player)
            player.CharacterAdded:Connect(function(character)
                wait(0.1)
                -- Example ESP with team color and player name LocalScript for Roblox
                -- Define team colors (replace with your desired colors)
                local teamColors = {
                    Team1 = Color3.fromRGB(255, 0, 0),      -- Red
                    Team2 = Color3.fromRGB(0, 0, 255),      -- Blue
                    Team3 = Color3.fromRGB(0, 255, 0),      -- Green
                    Team4 = Color3.fromRGB(255, 255, 0)     -- Yellow
                }

                -- Function to create ESP and display player names
                local function createESP(player)
                    local espBox = Instance.new("BoxHandleAdornment")
                    espBox.Size = Vector3.new(4, 5, 1)
                    espBox.Color3 = teamColors[player.Team.Name] or Color3.new(1, 1, 1) -- Default to white if team color not defined
                    espBox.AlwaysOnTop = true
                    espBox.ZIndex = 10
                    espBox.Adornee = player.Character:FindFirstChild("HumanoidRootPart")
                    espBox.Parent = player.Character

                    local nameTag = Instance.new("BillboardGui")
                    nameTag.Name = "NameTag"
                    nameTag.AlwaysOnTop = true
                    nameTag.Size = UDim2.new(0, 100, 0, 20)
                    nameTag.StudsOffset = Vector3.new(0, 2, 0) -- Adjust the offset to position the name tag
                    nameTag.Adornee = player.Character:FindFirstChild("Head") or player.Character:FindFirstChild("Torso")

                    local nameLabel = Instance.new("TextLabel", nameTag)
                    nameLabel.Name = "NameLabel"
                    nameLabel.Size = UDim2.new(1, 0, 1, 0)
                    nameLabel.BackgroundTransparency = 1
                    nameLabel.TextColor3 = teamColors[player.Team.Name] or Color3.new(1, 1, 1) -- Use team color, default to white
                    nameLabel.TextStrokeTransparency = 0.5
                    nameLabel.TextScaled = true
                    nameLabel.Text = player.Name

                    nameTag.Parent = player.Character
                end

                -- Function to create ESP for all existing players
                local function createAllESP()
                    for _, player in pairs(game.Players:GetPlayers()) do
                        if player ~= game.Players.LocalPlayer then
                            createESP(player)
                        end
                    end
                end

                -- Create ESP for all existing players when the script starts
                createAllESP()

                -- Create ESP for new players as they join
                if player ~= game.Players.LocalPlayer then
                    createESP(player)
                end
            end)
        end)
    end,
})
local Button = combat:CreateButton({
	Name = "테두리 ESP",
    Callback = function()
		Players.PlayerAdded:Connect(function(player)
			player.CharacterAdded:Connect(function(character)
				wait(0.1) -- 캐릭터가 로드될 시간을 기다립니다
				highlightPlayers()
			end)
		end)
		
		Players.PlayerRemoving:Connect(function(player)
			if player ~= LocalPlayer then
				local character = player.Character
				if character then
					local highlight = character:FindFirstChildOfClass("Highlight")
					if highlight then
						highlight:Destroy()
					end
				end
			end
		end)
		
		-- 처음에 모든 플레이어를 하이라이트합니다
		highlightPlayers()
		
		-- 로컬 플레이어의 캐릭터를 강조 표시하지 않도록 설정합니다
		LocalPlayer.CharacterAdded:Connect(function(character)
			local highlight = character:FindFirstChildOfClass("Highlight")
			if highlight then
				highlight:Destroy()
			end
		end)
	end,
 })
		

