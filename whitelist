local Input = Engine:CreateInput({
	Name = "ACS 2.0.1 플레이어 킬",
	PlaceholderText = "플레이어 이름 입력",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
		local Sound = Instance.new("Sound", game:GetService("SoundService"))
		Sound.SoundId = "rbxassetid://12221967"
		Sound:Play()
		local playerName = Text
		if game.Players:FindFirstChild(playerName) and playerName ~= game.Players.LocalPlayer.Name then
            game:GetService("ReplicatedStorage")["ACS_Engine"].Events.Suppression:FireServer(game.Players[playerName].Character.Humanoid, 100, 100, 0)
			Rayfield:Notify({
				Title = string.format("성공적으로 %s 킬했습니다.", playerName),
				Duration = 2,
				Image = 4483362458,
				Actions = {
					Ignore = {
						Name = "확인",
						Callback = function()
						end,
					},
				},
			})
		else
			Rayfield:Notify({
				Title = "플레이어에게 킬할 수 없습니다",
				Content = "이 서버에 존재하지 않거나 자기 자신일 경우 텔레포트가 불가능합니다.",
				Duration = 2,
				Image = 4483362458,
				Actions = {
					Ignore = {
						Name = "확인",
						Callback = function()
						end,
					},
				},
			})
		end
	end,
})

