	Name = "ACS 1.7.5 킬올",
	Callback = function()
		local Event = game.ReplicatedStorage:FindFirstChild("ACS_Engine.Eventos.Damage")
        -- local Event = game:GetService("ReplicatedStorage"):FindFirstChild("ACS_Engine")["Eventos"].Damage
		if Event then
			local Sound = Instance.new("Sound", game:GetService("SoundService"))
			Sound.SoundId = "rbxassetid://12221967"
			Sound:Play()
			for _, GivenPlayer in pairs(game.Players:GetPlayers()) do
				local Char = GivenPlayer.Character
				if Char and Char:FindFirstChild("Humanoid") then
					Event:FireServer(Char.Humanoid, Char.Humanoid.MaxHealth, 0, 0)
				end
			end
		else
			local Sound = Instance.new("Sound", game:GetService("SoundService"))
			Sound.SoundId = "rbxassetid://9066167010"
			Sound:Play()
